<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= title %>
    </title>
</head>

<body>
    <div>
        <ul>
            <% reports.forEach(function(report){ %>
                <li>
                    <a href="/reports/<%=report%>">
                        <%= report.replace(".html", "" ) %>
                    </a>
                </li>
                <% }); %>
        </ul>
    </div>
</body>

</html>